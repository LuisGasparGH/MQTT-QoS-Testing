{
    "logging":{
        "folder": "logs/#/",
        "main": "main-logger",
        "timestamp": "timestamp-logger"
    },
    "broker_address": "broker",
    "topics":{
        "main_topic": "mqtt_qos/main_topic/#",
        "begin_client": "mqtt_qos/begin_client",
        "void_run": "mqtt_qos/void_run",
        "finish_client": "mqtt_qos/finish_client",
        "client_done": "mqtt_qos/client_done"
    },
    "system_details":{
        "different_runs": 2,
        "run_repetitions": 5,
        "message_details":{
            "client_amount": 1,
            "msg_qos": 1,
            "msg_amount": 1000,
            "msg_size": 250000,
            "msg_freq": [30,50]
        }
    },
    "rtx_times": [10,15,20],
    "dumpcap":{
        "enable": true,
        "folder": "dumpcap/#/*C/",
        "filter": "tcp port 1883",
        "extension": ".pcapng",
        "buffer_size": 1536,
        "interface":{
            "client": "eth0",
            "server": "enp4s0"
        }
    }
}
