{
    "logging":{
        "folder": "logs/client-#/",
        "main": "main-logger",
        "timestamp": "timestamp-logger"
    },  
    "broker_address": "broker",
    "topics":{
        "main_topic": "mqtt_qos/main_topic/#",
        "begin_client": "mqtt_qos/begin_client",
        "finish_client": "mqtt_qos/finish_client",
        "client_done": "mqtt_qos/client_done"
    },
    "system_details":{
        "different_runs": 10,
        "message_details":{
            "client_amount": 1,
            "msg_qos": 0,
            "msg_amount": 1000,
            "msg_size": 2500,
            "msg_freq": 5
        }
    },
    "rtx_times": [25,55,130],
    "pyshark":{
        "folder": "wireshark/client-#/",
        "filter": "tcp port 1883",
        "extension": ".pcap",
        "interface": "eth0"
    }
}